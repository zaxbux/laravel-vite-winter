{
	"name": "innocenzi/laravel-vite",
	"description": "Vite integration for Laravel",
	"keywords": [
		"innocenzi",
		"laravel-vite"
	],
	"homepage": "https://github.com/innocenzi/laravel-vite",
	"license": "MIT",
	"authors": [
		{
			"name": "Enzo Innocenzi",
			"email": "enzo@innocenzi.dev",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.4|^8.0",
		"illuminate/contracts": "6.0 as 7.0",
		"spatie/laravel-package-tools": "^1.1",
		"guzzlehttp/guzzle": "^7.2"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.18",
		"pestphp/pest": "^1.0",
		"phpunit/phpunit": "^9.3"
	},
	"autoload": {
		"files": [
			"src/helpers.php"
		],
		"psr-4": {
			"Innocenzi\\Vite\\": "src",
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Innocenzi\\Vite\\Tests\\": "tests"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Innocenzi\\Vite\\ViteServiceProvider"
			],
			"aliases": {
				"Vite": "Innocenzi\\Vite\\ViteFacade"
			}
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
